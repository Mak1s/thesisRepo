<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>uploadServlet</servlet-name>
        <servlet-class>servlets.UploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadServletRDF</servlet-name>
        <servlet-class>servlets.UploadServletRDF</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadServlet1</servlet-name>
        <servlet-class>servlets.UploadServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadServletRDF1</servlet-name>
        <servlet-class>servlets.UploadServletRDF1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUser</servlet-name>
        <servlet-class>servlets.GetUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servlet_login</servlet-name>
        <servlet-class>servlets.servlet_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>classOnlyPost</servlet-name>
        <servlet-class>servlets.classOnlyPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>classPropertyPost</servlet-name>
        <servlet-class>servlets.classPropertyPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>removePost</servlet-name>
        <servlet-class>servlets.removePost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>translationPost</servlet-name>
        <servlet-class>servlets.translationPost</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>uploadServlet</servlet-name>
        <url-pattern>/UploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadServletRDF</servlet-name>
        <url-pattern>/UploadServletRDF</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadServlet1</servlet-name>
        <url-pattern>/UploadServlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadServletRDF1</servlet-name>
        <url-pattern>/UploadServletRDF1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUser</servlet-name>
        <url-pattern>/GetUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet_login</servlet-name>
        <url-pattern>/servlet_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>classOnlyPost</servlet-name>
        <url-pattern>/classOnlyPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>classPropertyPost</servlet-name>
        <url-pattern>/classPropertyPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>removePost</servlet-name>
        <url-pattern>/removePost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>translationPost</servlet-name>
        <url-pattern>/translationPost</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
